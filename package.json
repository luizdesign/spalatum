{
  "name": "spalatum",
  "version": "4.1.0",
  "description": "Render template and fragments",
  "main": "index.js",
  "engines": {
    "node": ">=7.10.0"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "codecov": "cat coverage/lcov.info | npx codecov",
    "lint": "eslint ./lib/** ./__*/** --ext .js",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:catho/spalatum.git"
  },
  "keywords": [
    "render",
    "fragments"
  ],
  "author": "Catho Online",
  "license": "GPL-3.0-only",
  "dependencies": {
    "bunyan": "^1.8.12",
    "cheerio": "^1.0.0-rc.2",
    "http-proxy": "^1.16.2",
    "moment": "^2.22.0",
    "superagent": "^3.8.2",
    "superagent-defaults": "^0.1.14",
    "superagent-proxy": "^1.0.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "mockdate": "^2.0.2"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "text-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
